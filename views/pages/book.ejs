<!DOCTYPE html>
<html>
<head>
	<title>gReads</title>
	<% include ../partials/head %>
</head>
<body>
<% include ../partials/header %>

<div class="container-fluid">
<div class="col-md-4">
	<% if (books) { %>
		<% books.forEach(function(book){ %>
		<div class="jumbotron">
			<div class="col-md-8">
				<div class="row">
					<div class="col-md-4">
						<img src="<%= book.img %>">
					</div>
					<div class="col-md-8">
					<h4><%= book.title %></h4>
					<p></p>
					</div>
				</div>
			</div>
		</div>
		<% }) %>
		<% } else { %>
			<div class="jumbotron">
				<h1>NO Books Yet</h1>
			</div>
		<% } %>
	</div>

<% include ../partials/footer %>
</body>
</html>